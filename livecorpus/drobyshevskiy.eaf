        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a78" ANNOTATION_REF="a43">
                <ANNOTATION_VALUE>so where could it</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a79" ANNOTATION_REF="a44">
                <ANNOTATION_VALUE>so again there should be a lot of coincidences</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a80" ANNOTATION_REF="a45">
